{
  "scripts": {
    "build": "node run.js",
    "test": "nyc --exclude-after-remap=false mocha ./src/**/test/*.js --timeout 20000 ",
    "generate": "SKIP_TESTS=true node run.js"
  },
  "nyc": {
    "include": [
      "src/**/lib/*.js"
    ],
    "exclude": [
      "src/**/test/*.js"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "extension": [
      ".js"
    ],
    "cache": true,
    "all": true
  },
  "dependencies": {
    "@accordproject/cicero-core": "0.9.1",
    "@accordproject/cicero-engine": "0.9.1",
    "adm-zip": "^0.4.11",
    "bulma": "^0.7.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-things": "^0.2.0",
    "composer-concerto": "0.31.2",
    "fs-extra": "^6.0.1",
    "lodash": "^4.17.10",
    "mocha": "^5.2.0",
    "moment": "^2.22.2",
    "nunjucks": "^3.1.3",
    "plantuml-encoder": "^1.2.5",
    "rimraf": "^2.6.2",
    "semver": "^5.5.0",
    "showdown": "^1.8.6",
    "uuid": "^3.2.1"
  },
  "name": "@accordproject/cicero-template-library",
  "description": "This repository host all Accord Project Cicero templates.",
